    "@testing-library/react-hooks": "8.0.1",
    "@testing-library/user-event": "12.8.1",
    "axios-mock-adapter": "1.19.0",
    "babel-loader": "8.2.5",
    "css-loader": "5.0.2",
    "eslint": "7.18.0",
    "eslint-config-htmlacademy": "2.1.0",
    "eslint-plugin-react": "7.22.0",
    "faker": "5.5.3",
